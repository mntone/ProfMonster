[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.swift]
indent_style = tab
indent_size = 4

[contents]
indent_style = space
indent_size = 4
insert_final_newline = false

[Contents.json]
indent_style = space
indent_size = 2

[*.xcscheme]
indent_style = space
indent_size = 3

[*.xcstrings]
indent_style = space
indent_size = 2
insert_final_newline = false
